rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    match /users/{uid} {
    	allow read: if request.auth.uid == uid;
      allow write;
    }

    match /consultations/{document=**}{
    	allow read,write;
    }
    
    match /chats/{document=**}{
    	allow read,write;
    }
  }
}